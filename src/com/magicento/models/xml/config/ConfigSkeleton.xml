    <frontend help="Configuration applied only to the frontend">
        <routers help="Inside this element you can create custom frontNames (the first part of the URL after the domain)">
            <___>
                <use/>
                <args>
                    <module/>
                    <frontName help="This is the value in the URL after the domain <br/>www.example.com/frontNameHere/controllerNameHere/actionNameHere"/>
